{
  "head": [
    {
      "element": "MetaFields"
    }
  ],
  "body": [
    {
      "element": "MainNavigation"
    },
    {
      "element": "div",
      "class": "md:mx-auto my-8 px-4 md:px-0 w-full lg:max-w-3xl flex flex-col gap-8",
      "children": [
        {
          "element": "h1",
          "class": "text-4xl",
          "==children": "match.title + ' posts'"
        },
        {
          "element": "BlogPosts",
          "__bind": "match.posts"
        },
        {
          "element": "Link",
          "class": "font-light",
          "children": "Back to topics",
          "attributes": {
            "href": "/topic/"
          }
        }
      ]
    },
    {
      "element": "MainFooter"
    },
    {
      "element": "Scripts",
      "__bind": "scripts"
    }
  ]
}
