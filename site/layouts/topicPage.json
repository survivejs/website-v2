{
  "head": [
    {
      "element": "MetaFields"
    }
  ],
  "body": [
    {
      "element": "MainNavigation"
    },
    {
      "element": "div",
      "class": "md:mx-auto my-8 px-4 md:px-0 w-full lg:max-w-3xl flex flex-col gap-8",
      "children": [
        {
          "element": "h1",
          "class": "text-4xl",
          "__children": "match.title"
        },
        {
          "element": "ul",
          "class": "",
          "__bind": "match.posts",
          "__children": [
            {
              "element": "li",
              "class": "",
              "children": [
                {
                  "element": "Link",
                  "class": "",
                  "__children": "title",
                  "attributes": {
                    "==href": "'/blog/' + slug + '/'"
                  }
                }
              ]
            }
          ]
        },
        {
          "element": "Link",
          "children": "Back to topics",
          "attributes": {
            "href": "/topic/"
          }
        }
      ]
    },
    {
      "element": "MainFooter"
    },
    {
      "element": "Scripts",
      "__bind": "scripts"
    }
  ]
}
