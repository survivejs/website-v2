{
  "body": [
    {
      "element": "rss",
      "attributes": {
        "version": "2.0"
      },
      "children": [
        {
          "element": "channel",
          "__bind": "blogPosts.content",
          "children": [
            {
              "element": "title",
              "__children": "meta.title"
            },
            {
              "element": "description",
              "__children": "meta.description"
            },
            {
              "element": "link",
              "__children": "meta.domain"
            },
            {
              "element": "lastBuildDate",
              "__children": "meta.built"
            },
            {
              "element": "item",
              "__children": [
                {
                  "element": "title",
                  "__children": "data.title"
                },
                {
                  "element": "description",
                  "__children": "data.description"
                },
                {
                  "element": "link",
                  "==children": "meta.domain + '/blog/' + data.slug + '/'"
                },
                {
                  "element": "guid",
                  "__children": "data.slug"
                },
                {
                  "element": "pubDate",
                  "__children": "data.date"
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}
