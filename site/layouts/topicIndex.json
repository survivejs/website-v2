{
  "head": [
    {
      "element": "MetaFields"
    }
  ],
  "body": [
    {
      "element": "MainNavigation"
    },
    {
      "element": "div",
      "class": "md:mx-auto my-8 px-4 md:px-0 w-full lg:max-w-3xl",
      "__bind": "topics.content",
      "children": [
        {
          "element": "h1",
          "class": "text-4xl mb-8",
          "children": "Topics"
        },
        {
          "element": "ul",
          "class": "flex flex-col gap-2",
          "__children": [
            {
              "element": "li",
              "class": "rounded",
              "children": [
                {
                  "element": "div",
                  "class": "rounded flex flex-col gap-2 p-2",
                  "children": [
                    {
                      "element": "Link",
                      "children": [
                        {
                          "element": "h2",
                          "class": "text-3xl",
                          "children": [
                            {
                              "element": "span",
                              "class": "pr-2",
                              "__children": "title"
                            },
                            {
                              "element": "span",
                              "class": "text-lg",
                              "==children": "'(' + count + ')'"
                            }
                          ]
                        }
                      ],
                      "attributes": {
                        "==href": "'/topic/' + slug + '/'"
                      }
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "element": "MainFooter"
    },
    {
      "element": "Scripts",
      "__bind": "scripts"
    }
  ]
}
