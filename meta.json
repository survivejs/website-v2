{
  "port": 3000,
  "amountOfBuildThreads": 5,
  "meta": {
    "domain": "https://survivejs.com",
    "language": "en",
    "siteName": "SurviveJS"
  },
  "paths": {
    "assets": "./assets",
    "components": "./site/components",
    "dataSources": "./site/dataSources",
    "layouts": "./site/layouts",
    "output": "./build",
    "routes": "./site/routes.json",
    "transforms": "./site/transforms",
    "twindSetup": "./site/twindSetup.ts"
  },
  "scripts": [
    {
      "type": "text/javascript",
      "src": "https://unpkg.com/sidewind@6.0.1/dist/sidewind.umd.production.min.js"
    }
  ],
  "features": {
    "extractCSS": false,
    "showEditorAlways": false
  }
}
